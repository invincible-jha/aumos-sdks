{
  "name": "@aumos/audit-trail",
  "version": "0.1.0",
  "description": "Immutable, hash-chained decision logging for AI agent governance",
  "license": "Apache-2.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist", "README.md", "LICENSE", "CHANGELOG.md"],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "vitest run"
  },
  "keywords": [
    "aumos",
    "ai-governance",
    "audit-log",
    "hash-chain",
    "agent-logging",
    "ai-agents",
    "compliance"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aumos-ai/aumos-sdks",
    "directory": "packages/audit-trail/typescript"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^1.5.0",
    "eslint": "^9.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
