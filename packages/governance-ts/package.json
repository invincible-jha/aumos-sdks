{
  "name": "@aumos/governance",
  "version": "0.1.0",
  "description": "TypeScript SDK for building governance-aware AI agent applications",
  "license": "BSL-1.1",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist", "README.md", "LICENSE", "CHANGELOG.md"],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "aumos",
    "ai-governance",
    "agent-governance",
    "trust",
    "budget-enforcement",
    "consent",
    "audit",
    "ai-agents"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aumos-ai/aumos-sdks",
    "directory": "packages/governance-ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^1.5.0",
    "eslint": "^9.0.0",
    "js-yaml": "^4.1.0",
    "@types/js-yaml": "^4.0.9"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
